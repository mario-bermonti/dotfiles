#+begin_src ipython :exports results :session :results output
import pandas as pd
data = pd.read_csv('data.csv')
data
 
#+end_src

#+RESULTS:


#+begin_src python :exports none :session   :results drawer output
import pandas_profiling as pp
d = pp.ProfileReport(data)
results = d.get_description()
for name, value in results["variables"].items():
    print(name, value)
    print()


#+end_src

